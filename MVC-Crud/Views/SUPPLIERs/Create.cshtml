@model MVC_Crud.ViewModel.SupplierViewModel

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

<div align="center">
    @using (Html.BeginForm("create", "SUPPLIERs", FormMethod.Post))
    {
        <table class="table">
            <tr>
                <td>
                    @Html.LabelFor(model => model.supplier.SUPLNO)
                </td>

                <td>
                    @Html.TextBoxFor(model => model.supplier.SUPLNO)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.supplier.SUPLNAME)
                </td>

                <td>
                    @Html.TextBoxFor(model => model.supplier.SUPLNAME)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.supplier.SUPLADDR)
                </td>

                <td>
                    @Html.TextBoxFor(model => model.supplier.SUPLADDR)
                </td>
            </tr>
            
            
            <tr>
                <td></td>

                <td>
                    <input type="submit" value="Save" />
                </td>
            </tr>
        </table>
    }
</div>
